<Window x:Class="Modeling.View.Main"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Modeling.View"
        xmlns:viewModel="clr-namespace:Modeling.ViewModel"
        mc:Ignorable="d"
        Title="Main" Height="300" Width="300">
    <Window.DataContext>
        <viewModel:MainViewModel/>
    </Window.DataContext>
    <Grid>
        <GroupBox Header="Initial parameters" DataContext="{Binding .InitialParametersModel}" HorizontalAlignment="Left" Margin="10,10,0,10" Width="135">
            <StackPanel>
                <Label Content="A"/>
                <TextBox Margin="5" DataContext="{Binding .A}"></TextBox>
                <Label Content="M"/>
                <TextBox Margin="5" DataContext="{Binding .M}"></TextBox>
                <Label Content="Start value"/>
                <TextBox Margin="5" DataContext="{Binding .InitialValue}"></TextBox>
            </StackPanel>
        </GroupBox>

    </Grid>
</Window>
